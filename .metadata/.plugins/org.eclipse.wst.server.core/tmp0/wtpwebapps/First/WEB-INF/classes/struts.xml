<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
   <constant name = "struts.devMode" value = "true" />
   <package name = "default" extends = "struts-default">
     
    <action name="autoCompleteAction" class="mypack.AutoCompleterAction" method="display">
    <result name="none">autocompleter.jsp</result>
    </action>
   
    <action name="result" class="mypack.Login">
    <result name="error"> Login.jsp</result>
    <result name="success">success.jsp</result>
    </action>
    
    <action name="loginaction" class="mypack.action.Loginlogin">
    <result name="success"> indexlogin.jsp</result>
    <result name="error"> loginlogin.jsp</result>
    </action>
    
    <action name="addEmployee" class="mypack.action.Employee">
    <result name="success">emp.jsp</result>
    <result name="error">emp.jsp</result>
    </action>
    
    <action name="logoutAction" class="mypack.action.Logout">
    <result name="success">indexlogin.jsp</result>
    </action>
    
   </package>
  </struts>
     